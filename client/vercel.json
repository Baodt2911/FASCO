{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/pages/index.html"
    },
    {
      "src": "/account",
      "dest": "/pages/account.html"
    },
    {
      "src": "/address",
      "dest": "/pages/address.html"
    },
    {
      "src": "/cart",
      "dest": "/pages/cart.html"
    },
    {
      "src": "/checkout",
      "dest": "/pages/checkout.html"
    },
    {
      "src": "/forgot-password",
      "dest": "/pages/forgotPassword.html"
    },
    {
      "src": "/orders",
      "dest": "/pages/orders.html"
    },
    {
      "src": "/product-detail",
      "dest": "/pages/productDetail.html"
    },
    {
      "src": "/shop",
      "dest": "/pages/shop.html"
    },
    {
      "src": "/signin",
      "dest": "/pages/signIn.html"
    },
    {
      "src": "/signup",
      "dest": "/pages/Signup.html"
    },
    {
      "src": "/404",
      "dest": "/pages/404.html"
    },
    {
      "src": "/(.*)",
      "dest": "/pages/404.html"
    }
  ]
}
